{
    "name": "savin/pcntl",
    "description": "Laravel package to work with the OS signals.",
    "license": "MIT",
    "keywords": ["Laravel", "PCNTL"],
    "require": {
        "illuminate/support": "~5",
        "ext-pcntl": "*"
    },
    "autoload": {
        "psr-4": {
            "Savin\\PCNTL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Savin\\PCNTL\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Savin\\PCNTL\\PCNTLServiceProvider"
            ],
            "aliases": {
                "PCNTL": "Savin\\PCNTL\\Facades\\PCNTL"
            }
        }
    }
}
